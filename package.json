{
  "name": "lynx-parser",
  "version": "0.2.1",
  "description": "A simple parser for application/lynx+json documents.",
  "main": "lib/index.js",
  "scripts": {
    "test-old": "babel-node test/index-tape.js | faucet; browserify test/index-tape.js -t babelify | tap-closer | smokestack --saucelabs | faucet",
    "test": "browserify test/index.js -t babelify > ./test/bundle.js; node test/server.js | bash test/saucelabs.sh",
    "build": "babel src -d lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lynx-json/lynx-parser.git"
  },
  "keywords": [
    "lynx"
  ],
  "author": "John Howes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lynx-json/lynx-parser/issues"
  },
  "homepage": "https://github.com/lynx-json/lynx-parser#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "faucet": "0.0.1",
    "smokestack": "^3.4.1",
    "tap-closer": "^1.0.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "content-type": "^1.0.2",
    "events": "^1.1.1"
  }
}
